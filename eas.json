{
  "build": {
    "production": {
      "env": {
        "EXPO_BUILD_ENV": "production"
      }
    },
    "test": {
      "releaseChannel": "test",
      "env": {
        "EXPO_BUILD_ENV": "test"
      }
    },
    "development": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_BUILD_ENV": "dev"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      },
      "ios": {
        "appleId": "hello@danielcross.dev",
        "appleTeamId": "3XQC7HQMCS"
      }
    },
    "test": {
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      },
      "ios": {
        "appleId": "hello@danielcross.dev",
        "appleTeamId": "3XQC7HQMCS"
      }
    }
  }
}
